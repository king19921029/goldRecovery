<view class="box"><view><block wx:for="{{arr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="chat_list"><uni-swipe-action vue-id="{{'1-'+index}}" options="{{options}}" data-event-opts="{{[['^click',[['action_click',['$event','$0'],[[['arr','',index]]]]]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['into_chatRoom',['$0'],[[['arr','',index]]]]]]]}}" class="tap_mask" catchtap="__e"><view class="list_box"><view class="list_left" data-username="{{item.username}}"><view class="list_pic"><image src="{{item.chatType=='groupchat'||item.chatType=='chatRoom'?'../../static/images/groupTheme.png':'../../static/images/theme@2x.png'}}"></image></view><view class="list_text"><text class="list_user">{{item.chatType=='groupchat'||item.chatType=='chatRoom'||item.groupName?item.groupName:item.username}}</text><block wx:if="{{item.msg.data[0].data}}"><text class="list_word">{{item.msg.data[0].data}}</text></block><block wx:if="{{item.msg.type=='img'}}"><text class="list_word">[图片]</text></block><block wx:if="{{item.msg.type=='audio'}}"><text class="list_word">[语音]</text></block></view></view><view class="list_right"><text data-username="{{item.username}}">{{item.time}}</text></view><block wx:if="{{item.unReadCount>0||item.unReadCount=='99+'}}"><view class="em-msgNum">{{item.unReadCount}}</view></block></view></view></uni-swipe-action></view></block><view style="height:1px;"></view></view></view>