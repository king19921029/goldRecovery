<view class="box"><view class="headers bg_all"></view><view class="com_block com_width"><block wx:if="{{isLogin}}"><view class="user_box com_width display_flex"><block wx:if="{{userInfo.header_pic_url}}"><image class="user_img" src="{{userInfo.header_pic_url}}" mode></image></block><block wx:else><image class="user_img" src="/static/imgs/fang_zw.jpg" mode></image></block><view class="user_info"><view>{{userInfo.nickname}}</view><text class="color_gray">{{userInfo.phone}}</text></view></view></block><block wx:else><view class="no_login"><view data-event-opts="{{[['tap',[['go_Testlogin',['$event']]]]]}}" class="btn" bindtap="__e">去登陆</view></view></block></view><view data-event-opts="{{[['tap',[['go_addressbook',['$event']]]]]}}" class="com_block com_width" bindtap="__e"><view class="com_list com_width display_flex space_between"><view class="display_flex"><image class="title_image" src="/static/imgs/lxwm.png" mode></image>通讯录</view><image class="right_icon" src="../../static/imgs/right_icon.png" mode></image></view></view><block wx:if="{{userInfo.vip_type==99}}"><view data-event-opts="{{[['tap',[['go_Testlogin',['$event']]]]]}}" class="com_block com_width" bindtap="__e"><view class="com_list com_width display_flex space_between"><view>内部测试入口</view><image class="right_icon" src="../../static/imgs/right_icon.png" mode></image></view></view></block><view data-event-opts="{{[['tap',[['go_moreIm',['$event']]]]]}}" class="com_block com_width" bindtap="__e"><view class="com_list com_width display_flex space_between"><view>群聊</view><image class="right_icon" src="../../static/imgs/right_icon.png" mode></image></view></view><view data-event-opts="{{[['tap',[['go_Msg',['$event']]]]]}}" class="com_block com_width" bindtap="__e"><view class="com_list com_width display_flex space_between"><view>单聊</view><image class="right_icon" src="../../static/imgs/right_icon.png" mode></image></view></view></view>